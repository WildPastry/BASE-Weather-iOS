    "expo-dev-client": "~3.3.8"
  },
  "devDependencies": {
    "@babel/core": "^7.23.9",
    "@reduxjs/toolkit": "^2.1.0",
    "@types/react-redux": "^7.1.33",
    "@types/react": "~18.2.48",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@wildpastry/eslint-config": "^1.6.4",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.33.2",
