{
  "name": "simple-weather",
  "version": "4.1.2",
  "main": "./app/App.tsx",
  "scripts": {
    "start": "NODE_ENV=development expo start",
    "android": "expo start --android",
    "build": "eas build -p android",
    "submit": "eas submit -p android",
    "ios": "expo start --ios",
    "lint": "eslint \"**/*.{js,jsx,ts,tsx}\"",
    "lint:fix": "eslint --fix \"**/*.{js,jsx,ts,tsx}\"",
    "format": "npx prettier --check \"**/*.{js,jsx,ts,tsx}\"",
    "format:fix": "npx prettier --write \"**/*.{js,jsx,ts,tsx}\"",
    "clean": "npx prettier --write \"**/*.{js,jsx,ts,tsx}\" && npm run lint:fix"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.0.0",
    "date-fns": "^3.6.0",
    "dotenv": "^16.4.5",
    "expo": "~50.0.17",
    "expo-dev-client": "~3.3.11",
    "expo-splash-screen": "^0.26.4",
    "expo-status-bar": "~1.11.1",
    "lottie-react-native": "^6.7.2",
    "react": "18.3.0",
    "react-native": "0.74.0",
    "react-native-safe-area-context": "^4.10.1",
    "react-redux": "^9.1.1",
    "expo-location": "~16.5.5",
    "expo-updates": "~0.24.12"
  },
  "devDependencies": {
    "@babel/core": "^7.24.4",
    "@reduxjs/toolkit": "^2.2.3",
    "@types/react": "~18.2.79",
    "@types/react-redux": "^7.1.33",
    "@typescript-eslint/eslint-plugin": "^7.7.1",
    "@wildpastry/eslint-config": "^1.6.5",
    "babel-plugin-transform-inline-environment-variables": "^0.4.4",
    "eslint": "^9.1.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.1",
    "prettier": "^3.2.5",
    "typescript": "^5.4.5"
  },
  "private": true
}
